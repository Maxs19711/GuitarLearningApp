<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#91E0FF"
    tools:context=".MainActivity3">

    <HorizontalScrollView
        android:id="@+id/horizontalScrollView"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_weight="1"
        android:scrollbars="none"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <Button
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text=""
                android:textSize="27dp" />

            <Button
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text=""
                android:textSize="27dp" />

            <Button
                android:id="@+id/c1"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="C"
                android:textSize="27dp" />

            <Button
                android:id="@+id/c2"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="C#"
                android:textSize="27dp" />

            <Button
                android:id="@+id/c3"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="D"
                android:textSize="27dp" />

            <Button
                android:id="@+id/c4"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="D#"
                android:textSize="27dp" />

            <Button
                android:id="@+id/c5"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="E"
                android:textSize="27dp" />

            <Button
                android:id="@+id/c6"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="F"
                android:textSize="27dp" />

            <Button
                android:id="@+id/c7"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="F#"
                android:textSize="27dp" />

            <Button
                android:id="@+id/c8"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="G"
                android:textSize="27dp" />

            <Button
                android:id="@+id/c9"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="G#"
                android:textSize="27dp" />

            <Button
                android:id="@+id/c10"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="A"
                android:textSize="27dp" />

            <Button
                android:id="@+id/c11"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="A#"
                android:textSize="27dp" />

            <Button
                android:id="@+id/c12"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="B"
                android:textSize="27dp" />

            <Button
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text=""
                android:textSize="27dp" />

            <Button
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text=""
                android:textSize="27dp" />


        </LinearLayout>

    </HorizontalScrollView>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <Button
            android:id="@+id/button1"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:background="@drawable/custom_button"
            android:text="Tools" />

        <Button
            android:id="@+id/button2"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:background="@drawable/custom_button"
            android:text="Tuner" />

        <Button
            android:id="@+id/button3"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:background="@drawable/custom_button"
            android:text="Games" />
    </LinearLayout>

    <View
        android:id="@+id/divider"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_weight="1"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/horizontalScrollView"
        app:layout_constraintTop_toTopOf="@id/horizontalScrollView"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="-3dp"
        tools:ignore="MissingConstraints" />

    <HorizontalScrollView
        android:id="@+id/horizontalScrollView2"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_weight="1"
        android:scrollbars="none"
        app:layout_constraintTop_toTopOf="@+id/divider"
        tools:layout_editor_absoluteX="-2dp"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:id="@+id/linearr"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <Button
                android:id="@+id/nth"
                android:layout_width="120dp"
                android:layout_height="60dp"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text=""
                android:textSize="27dp" />

            <Button
                android:id="@+id/m1"
                android:layout_width="120dp"
                android:layout_height="60dp"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="Major"
                android:textAllCaps="false"
                android:textSize="27dp" />

            <Button
                android:id="@+id/m2"
                android:layout_width="120dp"
                android:layout_height="60dp"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text="Minor"
                android:textAllCaps="false"
                android:textSize="27dp" />

            <Button
                android:id="@+id/nth2"
                android:layout_width="120dp"
                android:layout_height="60dp"
                android:background="@android:color/transparent"
                android:gravity="bottom|center_horizontal"
                android:text=""
                android:textAllCaps="false"
                android:textSize="27dp" />


        </LinearLayout>
    </HorizontalScrollView>

    <View
        android:id="@+id/divider5"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_weight="1"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/horizontalScrollView2"
        app:layout_constraintTop_toTopOf="@id/horizontalScrollView2"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="-1dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="76dp"
        android:layout_height="79dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/finger" />

    <ImageButton
        android:id="@+id/sound"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/speaker" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="241dp"
        android:layout_height="304dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/horizontalScrollView2"
        app:srcCompat="@mipmap/c1" />


</androidx.constraintlayout.widget.ConstraintLayout>